{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:react/recommended",
    "google",
  ],
  "plugins": ["@typescript-eslint", "react"],
  "rules": {
    "semi": ["error", "never"],
    "quotes": [2, "single"],
    "require-jsdoc": "off",
    "react/prop-types": "off"
  }
}